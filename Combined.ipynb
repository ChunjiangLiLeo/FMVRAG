      "source": [
        "# ✅ Agentic RAG Financial Assistant\n",
        "import os\n",
        "import re\n",
        "import glob\n",
        "import yfinance as yf\n",
        "from tavily import TavilyClient\n",
        "from langchain.chat_models import ChatOpenAI\n",
        "from langchain.agents import AgentExecutor, create_openai_functions_agent\n",
        "from langchain.tools import tool\n",
        "from langchain.prompts import ChatPromptTemplate, MessagesPlaceholder\n",
        "from langchain.document_loaders import PyPDFLoader, TextLoader, Docx2txtLoader\n",
        "from langchain.text_splitter import RecursiveCharacterTextSplitter\n",
        "from langchain_openai import OpenAIEmbeddings\n",
        "from langchain_community.vectorstores import FAISS\n",
        "\n",
        "# 🔐 Keys\n",
        "os.environ[\"TAVILY_API_KEY\"] = \"",
        "os.environ[\"OPENAI_API_KEY\"] = \""\n",
        "\n",
        "# ✅ Step 1: Load and Split Documents\n",
        "doc_dir = \"/content/drive/MyDrive/Financial RAG/Financial Documents\"\n",
        "all_files = glob.glob(doc_dir + \"/*\")\n",
        "documents = []\n",
        "for file in all_files:\n",
        "    if file.endswith(\".pdf\"):\n",
        "        loader = PyPDFLoader(file)\n",
        "    elif file.endswith(\".txt\"):\n",
        "        loader = TextLoader(file)\n",
        "    elif file.endswith(\".docx\"):\n",
        "        loader = Docx2txtLoader(file)\n",
        "    else:\n",
        "        continue\n",
        "    documents.extend(loader.load())\n",
        "\n",
        "splitter = RecursiveCharacterTextSplitter(chunk_size=500, chunk_overlap=100)\n",
        "chunked_docs = splitter.split_documents(documents)\n",
        "\n",
        "# ✅ Step 2: Embed and Save Vector DB\n",
        "embeddings = OpenAIEmbeddings(model=\"text-embedding-3-large\")\n",
        "texts = [doc.page_content for doc in chunked_docs]\n",
        "vectorstore = FAISS.from_texts(texts, embedding=embeddings)\n",
        "save_path = \"/content/drive/MyDrive/Financial RAG/FinancialVectorDB\"\n",
        "vectorstore.save_local(folder_path=save_path, index_name=\"financial_docs\")\n",
        "\n",
        "# ✅ Step 3: Load Vector DB as Retriever\n",
        "loaded_vectorstore = FAISS.load_local(\n",
        "    folder_path=save_path,\n",
        "    embeddings=embeddings,\n",
        "    index_name=\"financial_docs\",\n",
        "    allow_dangerous_deserialization=True\n",
        ")\n",
        "retriever = loaded_vectorstore.as_retriever()\n",
        "\n",
        "# ✅ Step 4: Tool: Retrieve from FAISS\n",
        "@tool\n",
        "def retrieve_documents(query: str) -> str:\n",
        "    \"\"\"Retrieve local vector-based financial knowledge.\"\"\"\n",
        "    docs = retriever.get_relevant_documents(query)\n",
        "    return \"\\n---\\n\".join([doc.page_content[:500] for doc in docs[:3]])\n",
        "\n",
        "# ✅ Step 5: Tool: FX Rate from Tavily\n",
        "@tool\n",
        "def get_latest_fx_rate_tavily(target_currency: str) -> str:\n",
        "    \"\"\"Get USD to target_currency FX rate via Tavily.\"\"\"\n",
        "    client = TavilyClient()\n",
        "    result = client.search(\n",
        "        query=f\"latest USD to {target_currency.upper()} exchange rate\",\n",
        "        search_depth=\"basic\", include_answer=True\n",
        "    )\n",
        "    return result.get(\"answer\", \"FX rate not found.\")\n",
        "\n",
        "# ✅ Step 6: Tool: Index Volatility from Yahoo or Tavily\n",
        "INDEX_TICKER_MAP = {\n",
        "    \"nasdaq\": \"^IXIC\", \"s&p 500\": \"^GSPC\", \"dow jones\": \"^DJI\",\n",
        "    \"nikkei 225\": \"^N225\", \"ftse 100\": \"^FTSE\", \"hang seng\": \"^HSI\",\n",
        "    \"nasdaq 100\": \"^NDX\"\n",
        "}\n",
        "\n",
        "@tool\n",
        "def get_index_volatility(index_name: str, period_days: int = 30) -> str:\n",
        "    \"\"\"Annualized volatility of a stock index over period_days.\"\"\"\n",
        "    index_key = index_name.lower().strip()\n",
        "    ticker = INDEX_TICKER_MAP.get(index_key)\n",
        "    if not ticker:\n",
        "        return f\"Unsupported index name: {index_name}\"\n",
        "    try:\n",
        "        df = yf.Ticker(ticker).history(period=f\"{period_days}d\")\n",
        "        if df.empty:\n",
        "            raise ValueError(\"No data found\")\n",
        "        df['returns'] = df['Close'].pct_change()\n",
        "        vol = df['returns'].std() * (252 ** 0.5)\n",
        "        return f\"📉 {index_name.upper()} volatility over {period_days}d: {vol:.4f}\"\n",
        "    except Exception as e:\n",
        "        try:\n",
        "            client = TavilyClient()\n",
        "            result = client.search(\n",
        "                query=f\"{index_name} index annualized volatility past {period_days} days\",\n",
        "                search_depth=\"basic\", include_answer=True\n",
        "            )\n",
        "            return f\"Fallback via Tavily:\\n{result.get('answer', 'No result')}\"\n",
        "        except Exception as ex:\n",
        "            return f\"❌ Failed both sources. Yahoo: {e}, Tavily: {ex}\"\n",
        "\n",
        "# ✅ Step 7: Tavily Web Search\n",
        "@tool\n",
        "def search_web(query: str) -> str:\n",
        "    \"\"\"Searches the web using Tavily API and returns top 3 snippets with source URLs.\"\"\"\n",
        "    client = TavilyClient()\n",
        "    results = client.search(query=query, max_results=3)\n",
        "    return \"\\n\\n\".join([f\"{r['content'][:300]}\\n(Source: {r['url']})\" for r in results['results']])\n",
        "\n",
        "\n",
        "# ✅ Step 8: Prompt + Agent\n",
        "prompt = ChatPromptTemplate.from_messages([\n",
        "    (\"system\", \"Use local docs first, then online sources. Cite info if possible.\"),\n",
        "    (\"human\", \"{input}\"),\n",
        "    MessagesPlaceholder(\"agent_scratchpad\")\n",
        "])\n",
        "\n",
        "llm = ChatOpenAI(model=\"gpt-4\", temperature=0)\n",
        "tools = [get_latest_fx_rate_tavily, get_index_volatility, retrieve_documents, search_web]\n",
        "agent = create_openai_functions_agent(llm=llm, tools=tools, prompt=prompt)\n",
        "agent_executor = AgentExecutor(agent=agent, tools=tools, verbose=True)\n",
        "\n",
        "# ✅ Step 9: Run Agent\n",
        "response = agent_executor.invoke({\"input\": \"How has USD/CNY affected the NASDAQ recently?\"})\n",
        "print(\"\\n\\n📊 Agent Response:\\n\", response[\"output\"])\n"
